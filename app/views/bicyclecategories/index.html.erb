<h1>Bicyclecategories#index</h1>
<p>Find me in app/views/bicyclecategories/index.html.erb</p>
<table>
	<tr>
		<th>ID</th>
		<th>name</th>
		<th>memo</th>
	</tr>

	<% @data.each do |obj| %>
	<tr>
		<td><%= obj.id %></td>
		<td><%= obj.name %></td>
