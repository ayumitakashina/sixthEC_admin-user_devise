<h1>Bicycleconfigs#edit</h1>
<p>設定の編集</p>
<% if @bicycleconfig.errors.any? %>
<ul>
	<% @bicycleconfig.errors.full_messages.each do |err| %>
	<li><%= err %></li>
	<% end %>
</ul>
<% end %>

<table>
	<%= form_for(@bicycleconfig, url:{controller:'bicycleconfigs',action:'edit'}) do |form| %>
<tr>
	<th>Name</th>
		<td><%= form.text_field :name %></td>
</tr>

<tr>
	<th>Value</th>
		<td><%= form.text_field :value %></td>
</tr>

<tr><th></th>
	<td><%= form.submit "送信" %></td>
</tr>

<% end %>
</table>
